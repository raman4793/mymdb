<div class="row">
  <div class="col s6 offset-s3">
    <%= search_form_for @search do |f| %>
      <div class="field">
        <%= f.text_field :name_cont, placeholder: 'Enter user name' %>
      </div>
    <% end %>
  </div>
</div>
<%= paginate @users %>
<div class="row">
  <% @users.each do |user| %>
    <div class="col s12 m4 l4">
      <%= render user %>
    </div>
  <% end %>
</div>