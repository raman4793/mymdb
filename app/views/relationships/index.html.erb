<div id="basic-tabs" class="section">
  <h4 class="header">Basic Tabs</h4>
  <div class="row">
    <div class="col s12">
      <div class="row">
        <div class="col s12">
          <ul class="tabs tab-demo z-depth-1">
            <li class="tab col s3"><a class="active" href="#test1">Followers</a>
            </li>
            <li class="tab col s3"><a href="#test2" class="">Followings</a>

            <li class="tab col s3"><a href="#test3" class="">Blocked users</a>
            <li class="indicator" style="right: 743px; left: 0px;"></li>
          </ul>
        </div>
        <div class="col s12">

          <div id="test1" class="col s12 active" style="">
            <% current_user.followers.each do |user| %>
              <div class="col s3">
                <%= render user %>
              </div>
            <% end %>
          </div>



          <div id="test2" class="col s12" style="display: none;">
            <% current_user.following_users.each do |user| %>
              <div class="col s3">
                <%= render user %>
              </div>
            <% end %>
          </div>

          <div id="test3" class="col s12 active" style="">
            <% current_user.blocks.each do |user| %>
              <div class="col s3">
                <%= render user %>
              </div>
            <% end %>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>